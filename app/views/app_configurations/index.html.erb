<p id="notice"><%= notice %></p>

<h1>Listing App Configurations</h1>

<table>
  <thead>
    <tr>
      <th>Configuration json</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @app_configurations.each do |app_configuration| %>
      <tr>
        <td><%= app_configuration.configuration_json %></td>
        <td><%= link_to 'Show', app_configuration %></td>
        <td><%= link_to 'Edit', edit_app_configuration_path(app_configuration) %></td>
        <td><%= link_to 'Destroy', app_configuration, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New App configuration', new_app_configuration_path %>
